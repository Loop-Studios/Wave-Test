fun main() {
    var a :i32 = 32;
    var b :i32 = 20;

    if (a > 30) {
        println("a({}) is greater than 30", a);
    } else {
        println("a({}) is less than or equal to 30", a);
    }

    if (b < a) {
        println("b({}) is less than a({})", b, a);
    } else if (b == a) {
        println("b({}) is equal to a({})", b, a);
    } else {
        println("b({}) is greater than a({})", b, a);
    }

}